	$(ERMREST_URL_AST_PYTHON_FILES:%=ermrest/url/%c) \
	$(ERMREST_URL_AST_PYTHON_FILES:%=ermrest/url/%o)

EDIT_FILES += $(ERMREST_URL_AST_PYTHON_FILES:%=ermrest/url/ast/%) \
	ermrest/url/ast/makefile-vars


